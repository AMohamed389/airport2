    <record id="hr_employee_extend_1" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//tree[1]/field[@name='name']" position="before">
                    <field name="x_staff_id"/>
                </xpath>
                <xpath expr="//tree[1]/field[@name='name']" position="after">
                    <field name="x_employee_status"/>
                    <field name="x_age"/>
                    <field name="gender"/>
                    <field name="x_religion"/>
                    <field name="x_receiving_work_date" optional="hide"/>
                    <field name="x_hiring_date" optional="hide"/>
                    <field name="x_end_of_service_date" optional="hide"/>
                    <field name="x_number_of_years" optional="hide"/>
                    <field name="x_job_degree" optional="hide"/>
                    <field name="x_job_degree_date" optional="hide"/>
                    <field name="x_pension_date" optional="hide"/>
                    <field name="x_education_certificate_level" optional="hide"/>
                    <field name="x_identity_issuer" optional="hide"/>
                    <field name="x_has_disability_condition" optional="hide"/>
                    <field name="x_military_start_date" optional="hide"/>
                    <field name="x_military_end_date" optional="hide"/>
                    <field name="x_military_status" optional="hide"/>
                    <field name="x_mother_name" optional="hide"/>
                    <field name="x_department_name" optional="show"/>
                    <field name="x_administration_name" optional="show"/>
                    <field name="x_public_administration_name" optional="show"/>
                    <field name="x_sector_name" optional="show"/>

                </xpath>
                <xpath expr="//field[@name='work_phone']" position="attributes">
                    <attribute name="optional">hide</attribute>
                </xpath>
                <xpath expr="//field[@name='work_email']" position="attributes">
                    <attribute name="optional">hide</attribute>
                </xpath>
                <xpath expr="//field[@name='parent_id']" position="attributes">
                    <attribute name="optional">hide</attribute>
                </xpath>
            </data>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" ref="hr.view_employee_tree"/>
        <field name="key" eval="False"/>
        <field name="mode">extension</field>
        <field name="model">hr.employee</field>
        <field name="name">hr.employee.tree customization</field>
        <field name="priority">99</field>
        <field name="type">tree</field>
    </record>

    <record id="hr_employee_extend_2" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <data>

                <xpath expr="//field[@name='coach_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                

                <xpath expr="//field[@name='department_id']" position="after">
                    <field name="x_sector_name" />
                    <field name="x_public_administration_name" />
                    <field name="x_administration_name" />
                    <field name="x_department_name" />
                </xpath>

                <xpath expr="//field[@name='job_title']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//button[@name='124']" position="after">
                    <field widget="statusbar" options="{'clickable': '1'}" name="x_employee_status"
                           string="Employee Status"/>
                </xpath>
                <xpath expr="//field[@name='mobile_phone']" position="before">
                    <field name="id" invisible="1"/>
                    <field name="x_staff_id" string="Staff Id"/>
                </xpath>
                <xpath expr="//field[@name='address_home_id']" position="before">
                    <field name="mobile_phone" position="move"/>
                    <field name="work_phone" position="move"/>
                    <field name="work_email" position="move"/>
                </xpath>
                <xpath expr="//field[@name='gender']" position="after">
                    <field name="x_religion" string="Religion"/>
                    <field name="x_age" string="Age"/>
                </xpath>
                <xpath expr="//field[@name='country_id']" position="after">
                    <field name="x_mother_name" string="Mother Name"/>
                </xpath>
                <xpath expr="//field[@name='certificate']" position="after">
                    <field name="x_education_certificate_level" string="Education Certificate Date"/>
                </xpath>
                <xpath expr="//field[@name='identification_id']" position="after">
                    <field name="x_identity_issuer" string="Identification Issuer"/>
                </xpath>
                <xpath expr="//form[1]/sheet[1]/notebook[1]" position="inside">
                    <page string="Job" name="job_page">
                        <group name="job_page_group_parent">
                            <group name="job_page_group_parent_left"/>
                        </group>
                    </page>
                </xpath>

                <xpath expr="//group[@name='job_page_group_parent_left']" position="inside">
                    <field name="x_qualitative_group_name" />
                    <xpath expr="//field[@name='job_id']" position="move"/>
                    <field name="x_job_degree" string="Job Degree"/>
                    <field name="x_job_degree_date" string="Job Degree Date"/>
                    <field name="x_supervision_job" string="Supervision Job"/>
                </xpath>
                <xpath expr="//group[@name='job_page_group_parent']" position="inside">
                    <group name="job_page_group_parent_right">
                        <field name="x_hiring_date" string="Hiring Date"/>
                        <field name="x_receiving_work_date" string="Receiving Work Date"/>
                        <field name="x_end_of_service_date" string="End Of Service Date"/>
                        <field name="x_oldest_hiring_date" string="Oldest Hiring Date"/>
                    </group>
                </xpath>

                <xpath expr="//group[@name='work_permit']" position="after">
                    <group name="disability_group" string="Disability">
                        <field name="x_has_disability_condition"/>
                        <field name="x_disability_id" string="Disability Id Number"/>
                    </group>
                </xpath>

                <xpath expr="//form[1]/sheet[1]/notebook[1]" position="inside">

                    <page string="Education Certificates" name="hr_education_certificates_page">
                        <field name="x_hr_education_certificate_id" context="{'default_x_employee_id': id}"/>
                    </page>

                    <page string="Insurance" name="insurance_page">
                        <group name="insurance_page_group_parent">
                            <group name="insurance_page_group_parent_left">
                                <field name="x_social_insurance_number" string="Social Insurance Number"/>
                                <field name="x_social_insurance_status" string="Social Insurance Status"/>
                            </group>
                            <group name="insurance_page_group_parent_right"/>
                        </group>
                    </page>
                    <page string="Pension" name="pension_page">
                        <group name="pension_page_group_parent">
                            <group name="pension_page_group_parent_left">
                                <field name="x_pension_date" string="Pension Date"/>
                            </group>
                            <group name="pension_page_group_parent_right"/>
                        </group>
                    </page>
                    <page string="Penalties" name="penalty_page">
                        <field name="x_employee_penalty" context="{'default_x_employee_id': id}"/>
                    </page>
                    <page string="Training" name="training_page">
                        <field name="x_employee_training" context="{'default_x_employee_id': id}" />
                            <!--<tree editable="bottom">
                                <field name="x_employee_id"/>
                                <field name="x_training"/>
                                <field name="x_training_code"/>
                                <field name="x_training_type"/>
                                <field name="x_training_grade"/>
                                <field name="x_start_date"/>
                                <field name="x_end_date"/>
                                <field name="x_status"/>
                            </tree>
                        </field>-->
                    </page>
                    <page string="Military" name="military_page">
                        <group>
                            <group name="military_page_group_parent_left">
                                <field name="x_military_status" string="Military Status"/>
                                <field name="x_military_start_date" string="Military Start Date"/>
                                <field name="x_military_end_date" string="Military End Date"/>
                            </group>
                            <group name="military_page_group_parent_right">
                            </group>
                        </group>
                    </page>
                    <page string="Delegation" name="delegation_page">
                        <group>
                            <group name="delegation_page_group_parent_left">
                                <field name="x_is_delegated" string="Is Delegated"/>
                                <field name="x_delegated_from" string="Delegated From"/>
                                <field name="x_delegated_to" string="Delegated To"/>
                            </group>
                            <group name="delegation_page_group_parent_right">
                            </group>
                        </group>
                    </page>
                    <page string="Loan" name="loan_page">
                        <group>
                            <group name="loan_page_group_parent_left">
                                <field name="x_is_loaned" string="Is Loaned"/>
                                <field name="x_loaned_from" string="Loaned From"/>
                                <field name="x_loaned_to" string="Loaned To"/>
                            </group>
                            <group name="loan_page_group_parent_right">
                            </group>
                        </group>
                    </page>
                    <page string="Notes" name="notes_page">
                        <field name="x_notes" placeholder="Type down your notes here..." />
                    </page>
                </xpath>

                <!-- <xpath expr="//field[@name='x_job_degree']" position="before">
                    <field name="x_qualitative_group_name" />
                </xpath> -->

            </data>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="key" eval="False"/>
        <field name="mode">extension</field>
        <field name="model">hr.employee</field>
        <field name="name">hr.employee.form customization</field>
        <field name="priority">99</field>
        <field name="type">form</field>
    </record>

